listen 80;
charset utf-8;

types {
  text/html					html htm shtml;
  text/css					css;
  image/jpeg				jpeg jpg;
  application/x-javascript	js;
  image/x-icon				ico;
  application/font-woff		woff;
}

location / {
  root $holoeverywhere_root;
  index index.html index.htm;
}

location /wiki {
  rewrite ^ https://github.com/ChristopheVersieux/HoloEverywhere/wiki? permanent;
}
